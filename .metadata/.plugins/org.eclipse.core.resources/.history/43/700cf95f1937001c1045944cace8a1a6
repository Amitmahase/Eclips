
public class Max3number {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a[] = { 2, 3, 4, 6, 7, 8, 9, 9, 6, 4 };
		int max = a[0];
		int max2 = a[0];
		//int max3 = a[0];

		for (int i = 0; i < a.length; i++) {

			if (max < a[i]) {

				max = a[i];

			}
			System.out.println("the max no. is -->" + max);
		}

		
	}
}

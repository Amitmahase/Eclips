public class parent
{

String s="parent";

parent()
{
System.out.println("Constructor parent")
}
}

public class Child
{
String s="child";

child()
{

System.out.println("constructor child");
System.out.println(s);
System.out.println(super.s);
System.out.println(this.s);

}
}

public class Test
{


public static void main(String[] args)


{

Child c=new Child();

}
}

package Encapsulation;

public class Employee {
	
	private String emp_name;
	private int emp_id;
	private String DOJ;
	
	public void setEmpname(String ename)
	{
		emp_name=ename;
	}
	public String getEmpname()
	{
		return emp_name;
	}
	
	public void setEmpid(String eid)
	{
		emp_id=eid;
	}
	public int getEmpid()
	{
		return emp_id;
	}
	
	

	
	
			
			
			
}

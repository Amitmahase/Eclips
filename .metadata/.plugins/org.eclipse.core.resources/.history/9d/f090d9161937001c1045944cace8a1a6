
public class Max3number {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a[] = { 2, 3, 4, 6, 7, 8, 9, 9, 6, 4 };
		int max = a[0];
		int max2 = a[0];
		int max3 = a[0];

		for (int i = 0; i < a.length; i++) {

			if (max < a[i]) {

				max = a[i];

			}
			System.out.println("the max no. is -->" + a[i]);
		}

		for (int i = 0; i < a.length; i++) {

			if (a[i] > max2 && max2 < max) {

				max2 = a[i];

			}
			System.out.println("the max2 no. is -->" + a[i]);
		}

	}
}

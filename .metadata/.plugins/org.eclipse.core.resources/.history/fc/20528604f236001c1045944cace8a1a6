package MAxtwonoinArray;

import java.util.Arrays;

public class MAxTwoNumber {

	public static void main(String[] args) {

		
		
	String s="i am a software engineer  having 4.5 years of experience";
	
	int count=0;
	s=s.trim();
	
	for(int i=0;i<s.length();i++)
	{
		
		if(s.charAt(i)==' ' && s.charAt(i+1) !=' ')
		{
			count++;
		}
		
		
	}
	
	System.out.println("words in the given string i-->" + (count+1));
		
		
}
}

public class SuperAndThis {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		class p
		{
			
			String s="parent variable";
			
		}
		
		class C extends p
		{
			
			String s="child variable";
			
			public void m1()
			{
				
				System.out.println(s);
			}
			
			public void main(String[] args)
			{
				C c1=new C();
				
				c1.m1();
			}
			
			
			
		}

	}

}

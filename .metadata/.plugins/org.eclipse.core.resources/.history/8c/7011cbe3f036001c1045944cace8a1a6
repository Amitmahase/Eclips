package MAxtwonoinArray;

import java.util.Arrays;

public class MAxTwoNumber {

	public static void main(String[] args) {

		
		
		String s="i am hero %%%%%% open %%()&#&#";
		
		s=s.replaceAll("[^a-zA-Z0-9]", "");
		System.out.println(s);
		
		s=s.replace('o', 'a');
		System.out.println(s);
		
		
}
}